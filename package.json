{
  "name": "docker-compose-cache-action",
  "version": "1.7.0",
  "description": "Dynamically caches Docker images defined in a docker-compose.yml file using TypeScript.",
  "keywords": [
    "github",
    "actions",
    "docker",
    "docker-compose",
    "cache"
  ],
  "homepage": "https://github.com/seijikohara/docker-compose-cache-action#readme",
  "bugs": {
    "url": "https://github.com/seijikohara/docker-compose-cache-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seijikohara/docker-compose-cache-action.git"
  },
  "license": "MIT",
  "author": "Seiji Kohara <sage.0118@gmail.com> (https://github.com/seijikohara)",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "package": "npm run clean && ncc build src/main.ts -o dist --source-map --license licenses.txt",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "lint:fix": "eslint \"{src,tests}/**/*.ts\" --fix",
    "format": "prettier --write . --ignore-unknown",
    "format:check": "prettier --check . --ignore-unknown",
    "fix": "npm run format && npm run lint:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:check": "npx npm-check-updates"
  },
  "dependencies": {
    "@actions/cache": "^5.0.1",
    "@actions/core": "^2.0.1",
    "@actions/exec": "^2.0.0",
    "date-fns": "^4.1.0",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.21",
    "@types/node": "^25.0.2",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}

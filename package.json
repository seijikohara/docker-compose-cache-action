{
  "name": "docker-compose-cache-action",
  "version": "1.7.0",
  "description": "Dynamically caches Docker images defined in a docker-compose.yml file using TypeScript.",
  "keywords": [
    "github",
    "actions",
    "docker",
    "docker-compose",
    "cache"
  ],
  "homepage": "https://github.com/seijikohara/docker-compose-cache-action#readme",
  "bugs": {
    "url": "https://github.com/seijikohara/docker-compose-cache-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seijikohara/docker-compose-cache-action.git"
  },
  "license": "MIT",
  "author": "Seiji Kohara <sage.0118@gmail.com> (https://github.com/seijikohara)",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "package": "npm run clean && ncc build src/main.ts -o dist --source-map --license licenses.txt",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "lint:fix": "eslint \"{src,tests}/**/*.ts\" --fix",
    "format": "prettier --write . --ignore-unknown",
    "format:check": "prettier --check . --ignore-unknown",
    "fix": "npm run format && npm run lint:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@actions/cache": "^4.0.3",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "date-fns": "^4.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.35.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
